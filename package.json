{
  "name": "debug-level",
  "version": "0.3.0",
  "description": "debug with levels",
  "keywords": [
    "debug",
    "level"
  ],
  "homepage": "https://github.com/commenthol/debug-level#readme",
  "bugs": {
    "url": "https://github.com/commenthol/debug-level/issues"
  },
  "license": "MIT",
  "author": "commenthol <commenthol@gmail.com>",
  "main": "lib",
  "module": "src",
  "directories": {
    "lib": "lib",
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/debug-level.git"
  },
  "scripts": {
    "all": "npm run clean && npm run lint && npm run build && npm test",
    "build": "npm run clean && babel -d lib src",
    "build:example": "cd examples/app && webpack",
    "clean": "rimraf lib coverage",
    "coverage": "nyc -r lcov -r text npm test",
    "coveralls": "npm run build && nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint '**/*.js'",
    "prepublishOnly": "npm run all",
    "start:example": "node examples/app/server.js",
    "test": "mocha",
    "test:karma": "DEBUG_LEVEL=DEBUG karma start"
  },
  "browser": "./lib/browser.js",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "extends": "standard"
  },
  "dependencies": {
    "asyncc": "^2.0.1",
    "chalk": "^2.2.0",
    "json-stringify-safe": "^5.0.1",
    "map-lru": "^1.0.0",
    "ms": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.2.0",
    "nyc": "^11.3.0",
    "rimraf": "^2.4.0",
    "sinon": "^5.0.10",
    "webpack": "^4.10.2"
  },
  "engines": {
    "node": ">=4"
  }
}
