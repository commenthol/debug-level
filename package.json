{
  "name": "debug-level",
  "version": "0.3.1",
  "description": "debug with levels",
  "keywords": [
    "debug",
    "level"
  ],
  "homepage": "https://github.com/commenthol/debug-level#readme",
  "bugs": {
    "url": "https://github.com/commenthol/debug-level/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/debug-level.git"
  },
  "license": "MIT",
  "author": "commenthol <commenthol@gmail.com>",
  "main": "lib",
  "module": "src",
  "browser": "./lib/browser.js",
  "directories": {
    "lib": "lib",
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "all": "npm run clean && npm run lint && npm run build && npm test",
    "build": "npm run clean && babel -d lib src",
    "build:example": "cd examples/app && webpack",
    "clean": "rimraf lib coverage",
    "coverage": "nyc -r lcov -r text npm test",
    "coveralls": "npm run build && nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint '**/*.js'",
    "prepublishOnly": "npm run all",
    "start:example": "node examples/app/server.js",
    "test": "mocha",
    "test:karma": "DEBUG_LEVEL=DEBUG karma start"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "mocha": true,
      "node": true
    },
    "extends": "standard",
    "rules": {
      "node/no-deprecated-api": "warn"
    }
  },
  "dependencies": {
    "asyncc": "^2.0.4",
    "chalk": "^4.1.0",
    "json-stringify-safe": "^5.0.1",
    "map-lru": "^1.1.0",
    "ms": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.1.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.2",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "webpack": "^4.43.0"
  },
  "engines": {
    "node": ">=4"
  }
}
